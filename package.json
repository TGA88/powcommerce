{
  "name": "powcommerce",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build:all": "nx run-many --target=build",
    "build:changed": "nx affected --target=build --base HEAD~1 --head HEAD",
    "build": "nx build",
    "lint:all": "nx run-many --target=lint",
    "lint:changed": "nx affected --target=lint --base HEAD~1 --head HEAD",
    "lint": "nx lint",
    "test:all": "nx run-many --target test",
    "test:changed": "nx affected --target=test --base HEAD~1 --head HEAD",
    "test": "nx test",
    "build-img:all": "nx run-many --target docker-build",
    "build-img:changed": "nx affected --target docker-build --base HEAD~1 --head HEAD",
    "build-img": "nx docker-build",
    "nxcache:clean": "rm -rf node_modules/.cache",
    "compose-up": "docker-compose up",
    "compose-down": "docker-compose down",
    "commit": "git-cz",
    "offline:debug": "export SLS_DEBUG=* && export IS_OFFLINE='true' && node ./node_modules/serverless/bin/serverless.js offline start --httpPort 9090 --host 0.0.0.0 --skipCacheInvalidation --noPrependStageInUrl --stage local",
    "offline": "export SLS_DEBUG=* &&  serverless offline start --config serverless.offline.yml --stage local",
    "init:version": "yarn nx g @jscutlery/semver:install",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@nrwl/next": "15.5.3",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jscutlery/semver": "^2.29.3",
    "@nrwl/cypress": "15.5.3",
    "@nrwl/esbuild": "15.5.3",
    "@nrwl/eslint-plugin-nx": "15.5.3",
    "@nrwl/jest": "15.5.3",
    "@nrwl/linter": "15.5.3",
    "@nrwl/node": "^15.5.3",
    "@nrwl/react": "15.5.3",
    "@nrwl/rollup": "15.5.3",
    "@nrwl/web": "15.5.3",
    "@nrwl/workspace": "15.5.3",
    "@rollup/plugin-url": "^7.0.0",
    "@svgr/rollup": "^6.1.2",
    "@swc/core": "^1.2.173",
    "@swc/helpers": "~0.4.11",
    "@swc/jest": "0.2.20",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "28.1.1",
    "cypress": "^12.2.0",
    "esbuild": "^0.15.7",
    "eslint": "~8.15.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "nx": "15.5.3",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "sass": "1.55.0",
    "swc-loader": "0.1.15",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
